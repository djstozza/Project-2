<p id="notice"><%= notice %></p>

<h2><%= @current_user.name %>'s Items</h2>

<% @items.each do |item| %>
  <ul>
    <li>
      <ul>
        <li><%= link_to item.name, item %></li>
        <li><%= item.image %></li>
        <li><%= item.created_at.strftime "%e %B, %Y" %></li>
      </ul>
    </li>
  </ul>
<% end %>
 


<br>

<%= link_to 'New Item', new_item_path %>
